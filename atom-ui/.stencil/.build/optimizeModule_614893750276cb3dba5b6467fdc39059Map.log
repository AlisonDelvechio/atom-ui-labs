{"version":3,"names":["menuAccordionCss","MenuAccordion","heading","device","expanded","isExpanded","componentWillLoad","this","toggleAccordion","renderExpandIcon","getExpandLessIcon","getExpandMoreIcon","h","class","width","height","viewBox","d","render","key","onClick"],"sources":["src/components/menu-accordion/menu-accordion.scss?tag=menu-accordion","src/components/menu-accordion/menu-accordion.tsx"],"sourcesContent":["@use '../../styles/variables' as *;\n@use 'sass:color';\n\n.accordion {\n  border: 1px solid $gray-300;\n  border-radius: 0.5rem;\n  overflow: hidden;\n  background-color: $gray-50;\n  box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.2);\n\n  &.mobile {\n    font-size: 0.9rem;\n    width: 730px;\n  }\n\n  &.web {\n    font-size: 1rem;\n  }\n\n  &.expanded {\n    border-color: color.adjust($gray-300, $lightness: -10%);\n  }\n\n  .accordion-header {\n    all: unset;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    width: 97%;\n    padding: 1rem;\n    background-color: $gray-100;\n    cursor: pointer;\n\n    .title {\n      font-weight: bold;\n      line-height: 1.2;\n    }\n\n    .icon {\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      height: 1.5rem;\n      width: 1.5rem;\n      fill: currentColor;\n    }\n  }\n\n  .accordion-content {\n    padding: 0.5rem 1rem;\n    background-color: white;\n\n    .accordion-item {\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      padding: 0.75rem 0;\n      border-bottom: 1px solid $gray-100;\n      color: inherit;\n      text-decoration: none;\n      transition: background-color 0.2s ease;\n\n      span:first-child {\n        margin-right: 0.5rem;\n      }\n\n      .icon {\n        font-size: 1.2rem;\n      }\n\n      // &:hover {\n      //   background-color: $gray-50;\n      // }\n\n      &:last-child {\n        border-bottom: none;\n      }\n    }\n  }\n}","import { Component, Prop, State, h } fro